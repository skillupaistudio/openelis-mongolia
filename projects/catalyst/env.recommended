# Copy this file to .env for local development.

# LLM (LM Studio) configuration
LLM_BASE_URL=http://localhost:1234
LLM_MODEL=local-model

# Ports
GATEWAY_PORT=8000
ROUTER_PORT=9100
CATALYST_AGENT_PORT=9101
MCP_PORT=9102

# A2A endpoint URLs (Router -> Catalyst)
CATALYST_ROUTER_URL=http://localhost:9100
CATALYST_AGENT_URL=http://localhost:9101

# MCP endpoint URL (Catalyst -> MCP)
MCP_URL=http://localhost:9102

# MCP table allowlist (comma-separated, default = minimal non-PHI profile)
# Default includes: test, test_section, analyte, method, panel, panel_item,
# type_of_sample, type_of_test_result, unit_of_measure, dictionary,
# dictionary_category, status_of_sample
MCP_ALLOWED_TABLES=

# Database (M1+ - not used in M0.0)
MCP_DB_ENABLED=false
MCP_DB_HOST=db.openelis.org
MCP_DB_PORT=5432
MCP_DB_NAME=clinlims
MCP_DB_USER=catalyst_schema_reader
MCP_DB_PASSWORD=
MCP_DB_SCHEMA=clinlims
